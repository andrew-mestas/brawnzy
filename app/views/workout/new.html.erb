<h1>New</h1>

<h1>Workout#create</h1>

<%= form_for :workout do |f| %>
  <%= f.label :type %>:
  <%= f.text_field :type %><br />

  <%= f.label :name %>:
  <%= f.text_field :name %><br />

  <%= f.label :sets %>:
  <%= f.text_field :sets %><br />

  <%= f.label :time %>:
  <%= f.text_field :time %><br />

  <%= f.submit %>
<% end %>